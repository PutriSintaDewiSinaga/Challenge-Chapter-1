    <script>
        var saldo = 10000000
        var saldoakhir = window.prompt('Masukan jumlah saldo : ')
        function tambahsaldo(saldo, saldoakhir) {
            return saldo + saldoakhir
        }
        function kurangsaldo(saldo, saldoakhir) {
            return saldo - saldoakhir
        }
    </script>
